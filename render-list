const list = [1,2,3,4,5,6,7,8,9,0];

export default function Calculator(){
  return <SafeAreaView>
    <View style={{flexDirection:'row'}}>
      {list.map((item , index)=>(
        <Text style={{backgroundColor:'black',color:'white'}} key={index}>
          {item}
        </Text>
      ))}
    </View>
  </SafeAreaView>
}
